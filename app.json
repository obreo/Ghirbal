{
  "expo": {
    "name": "Ghirbal",
    "slug": "ghirbal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ghirbal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ghirbal.app",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "youtube",
          "github",
          "twitter",
          "instagram",
          "fb",
          "maps",
          "spotify",
          "linkedin",
          "reddit",
          "telegram",
          "whatsapp"
        ],
        "NSCameraUsageDescription": "Allow ABrowser to access your camera for video calls and live streaming",
        "NSMicrophoneUsageDescription": "Allow ABrowser to access your microphone for voice calls and audio recording",
        "NSLocationWhenInUseUsageDescription": "Allow ABrowser to access your location for location-based services",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow ABrowser to access your location for location-based services",
        "NSPhotoLibraryUsageDescription": "Allow ABrowser to access your photos for sharing and uploading",
        "NSPhotoLibraryAddUsageDescription": "Allow ABrowser to save photos to your library"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ghirbal.app",
      "softwareKeyboardLayoutMode": "resize",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "http"
            },
            {
              "scheme": "https"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#1a1a1a"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "expo-web-browser",
      "./plugins/withWebViewPermissionControl.js"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "a2eb2e5f-3bfa-4bda-861c-e93f28fb6372"
      }
    },
    "owner": "khadeibr"
  }
}